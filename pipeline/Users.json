{
	"name": "Users",
	"properties": {
		"activities": [
			{
				"name": "Get User password from Azure Key Vault",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": true,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": "https://rebrickabledevakv.vault.azure.net/secrets/Rebrickable-User-Password?api-version=7.4",
					"authentication": {
						"type": "MSI",
						"resource": "https://vault.azure.net/"
					}
				}
			}
		],
		"annotations": []
	}
}